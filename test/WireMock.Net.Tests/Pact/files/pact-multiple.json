{
  "consumer": {
    "name": "Something API Consumer Multiple"
  },
  "interactions": [
    {
      "providerState": "A GET request to retrieve the something",
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "POST",
        "path": "/tester",
        "query": "q1=test&q2=ok"
      },
      "response": {
        "body": {
          "id": "tester",
          "firstName": "Totally",
          "lastName": "Awesome"
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "status": 200
      }
    },
    {
      "providerState": "A Post request to add the something",
      "request": {
        "headers": {
          "Accept": "application/json"
        },
        "method": "POST",
        "path": "/add",
        "body": {
          "Id": "1",
          "FirstName": "Totally"
        }
      },
      "response": {
        "body": {
          "id": "1",
          "firstName": "Totally"
        },
        "status": 303
      }
    }
  ],
  "provider": {
    "name": "Something API"
  }
}